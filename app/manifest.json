{
    "name": "nifty id switcher",
    "version": "0.0.464",
    "manifest_version": 2,
    "description": "nifty id switcher",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "scripts/chromereload.js",
            "scripts/background.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon-19.png",
            "38": "images/icon-38.png"
        },
        "default_title": "nifty id switcher",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "permissions": [
        "https://www.nifty.com/",
        "http://www.nifty.com/",
        "https://sso.nifty.com/"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}